{
  "name": "ZhihuDaily",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-open": "open ios/*.xcodeproj",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "lint": "eslint src test *.js",
    "test": "mocha --recursive --compilers js:babel-core/register -r test/setup",
    "cover": "nyc npm test",
    "remotedev": "remotedev-debugger --injectserver",
    "postinstall": "npm run remotedev && rndebugger-open",
    "android-release": "code-push release-react react-native-redux-zhihudaily android --des '测试热更新，改变标题栏颜色'",
    "ios-release": "code-push release-react react-native-redux-zhihudaily-ios ios --des '测试热更新，改变标题栏颜色'",
    "android-history": "code-push deployment history react-native-redux-zhihudaily Staging",
    "ios-history": "code-push deployment history react-native-redux-zhihudaily-ios Staging"
  },
  "remotedev": {
    "hostname": "localhost",
    "port": 5678
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.17.13",
    "react": "15.4.2",
    "react-native": "0.40.0",
    "react-native-code-push": "^1.16.1-beta",
    "react-native-drawer-layout": "^1.0.0",
    "react-native-easy-grid": "^0.1.6",
    "react-native-nav": "^1.1.4",
    "react-native-root-toast": "^1.0.3",
    "react-native-storage": "^0.1.3",
    "react-native-swiper": "^1.4.8",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-react-native": "^1.4.0",
    "chai": "^3.5.0",
    "enzyme": "^2.0.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "immutable-devtools": "0.0.7",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "node-fetch": "^3.1.1",
    "nyc": "^8.1.0",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-native-debugger-open": "^0.3.0",
    "react-native-mock": "^0.2.3",
    "remote-redux-devtools": "^0.4.1",
    "remote-redux-devtools-on-debugger": "^0.6.0",
    "sinon": "^1.17.2"
  }
}
